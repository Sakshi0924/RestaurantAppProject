@model RestaurantEFDbFirstApproach.Models.RestaurantTable
@{
    ViewBag.Title = "Details";
}

<h2>Details of Single Restaurant Item</h2>

<table class="table">
    <tr>
        <th>Restaurant Id:</th>
        <td>@Model.RestaurantId</td>
    </tr>
    <tr>
        <th>Name:</th>
        <td>@Model.Name</td>
    </tr>
    <tr>
        <th>Cusine:</th>
        <td>@Model.Cusine</td>
    </tr>
</table>
<a href="/Home/Index" class="btn btn-info">Back</a>
